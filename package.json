{
    "name": "react-parcel",
    "version": "1.0.0",
    "description": "react-parcel",
    "main": "index.js",
    "scripts": {
        "start": "parcel index.html --open --port 4000",
        "build": "ttsc && parcel build index.html --no-source-maps",
        "deploy": "npm run build && firebase deploy"
    },
